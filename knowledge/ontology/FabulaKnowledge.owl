          <owl:ObjectProperty rdf:about="#motivates"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#SettingElement"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Goal"/>
    <owl:disjointWith rdf:resource="#Outcome"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Perception"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Action"/>
    <owl:disjointWith rdf:resource="#Event"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#psi_causes"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#InternalElement"/>
              <owl:Class rdf:about="#Goal"/>
              <owl:Class rdf:about="#Outcome"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#FabulaElement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasContent"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#ContentGraph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Anything internal to a character, like a belief, or an emotion, feeling, etc.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#enables"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Goal"/>
              <owl:Class rdf:about="#Action"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="WingFly">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Fly"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SwitchOn">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
