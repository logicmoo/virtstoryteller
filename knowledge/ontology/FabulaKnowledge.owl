          <owl:Class rdf:about="#Goal"/>
          <owl:Class rdf:about="#Action"/>
          <owl:Class rdf:about="#InternalElement"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
