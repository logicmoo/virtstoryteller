    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Event"/>
          <owl:Class rdf:about="#Perception"/>
          <owl:Class rdf:about="#InternalElement"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
