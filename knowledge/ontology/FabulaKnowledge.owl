        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Event"/>
              <owl:Class rdf:about="#Perception"/>
              <owl:Class rdf:about="#BeliefElement"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Event"/>
  </owl:Class>
  <owl:Class rdf:about="#Attaching">
